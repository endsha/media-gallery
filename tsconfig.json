{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@screens/*": ["./src/screens/*"],
      "@utils/*": ["./src/utils/*"],
      "@assets/*": ["./src/assets/*"],
      "@constants/*": ["./src/constants/*"],
      "@customTypes/*": ["./src/types/*"],
      "@actionHooks": ["src/actionHooks"],
      "@services": ["./src/services"],
      "@config": ["./src/config"],
      "@languages/*": ["./src/languages/*"],
      "@redux/*": ["./src/redux/*"]
    }
  },
  "extends": "@react-native/typescript-config/tsconfig.json"
}
